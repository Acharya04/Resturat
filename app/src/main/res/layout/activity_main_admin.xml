<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainAdmin">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#4DF564">

        <Button
            android:id="@+id/adminbtn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Event Booking Check"
            android:layout_marginTop="300px">

        </Button>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Online Order Check"
            android:id="@+id/adminbtn2"
            android:layout_below="@id/adminbtn1"
            android:layout_marginTop="40dp" >



        </Button>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/adminbtn3"
            android:text="Logout"
            android:layout_below="@id/adminbtn2"
            android:layout_marginTop="50dp"/>

    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>